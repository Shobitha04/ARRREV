void ReversedArray(int arr[], int n)
{
  int low=0, high=n-1;
  while(low<high)
    {
        temp = arr[low];
        arr[low]=arr[high];
        arr[high]=temp;
        low++;
        high--;

      }

  5     6   21   7   9
  low=0              high=4

  9   6   21   7      5
     low=1    high=3

  9   7   21   6   5
          low=high=2 
  stop
  reversed!
