class RemoveElement{
  public int removeEle(int[]nums, int val){
    int k=0;// Initialize k to count elements not equal to val
    for(int i=0;i<nums.length;i++){
      if(nums[i]!=val){// If current element is not equal to val
        nums[k]=nums[i]; // Place it at index k
        k++;
                       }
      }
    }
  return k;
  }
